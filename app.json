{
	"expo": {
		"name": "Offline Password Store",
		"slug": "offlinepasswordstore",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/images/logo-key-1024.png",
		"primaryColor": "#589CA9",
		"description": "",
		"splash": {
			"image": "./assets/images/logo-splash.png",
			"backgroundColor": "#F2F1EF"
		},
		"plugins": [
			"expo-localization",
			"@config-plugins/react-native-blob-util",
			"@config-plugins/react-native-pdf",
			[
				"expo-document-picker",
				{
					"iCloudContainerEnvironment": "Production"
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "6ce6e53e-664b-4a54-a4d1-944b78f70e9b"
			}
		},
		"android": {
			"package": "io.slamcode.offlinepasswordstore",
			"data": {
				"mimeType": "application/opm"
			}
		},
		"ios": {
			"usesIcloudStorage": true
		}
	}
}
